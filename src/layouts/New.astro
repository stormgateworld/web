---
import Root from "./Root.astro"
import MainNav from "../components/layout/MainNav.astro"
export interface Props {
  title: string
}

const { title } = Astro.props
---

<Root title={title}>
  <MainNav />
  <slot />
  <div class="py-32 text-center border-t mt-4 md:mt-8 border-gray-900">Footer</div>
</Root>
